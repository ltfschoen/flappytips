{
    "name": "server",
    "version": "2.1.0",
    "private": true,
    "dependencies": {
        "@babel/cli": "^7.21.0",
        "@babel/core": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "@polkadot/api": "^10.0.1",
        "@polkadot/api-contract": "^10.0.1",
        "@polkadot/typegen": "^10.0.1",
        "axios": "^1.3.2",
        "bn.js": "^5.2.1",
        "body-parser": "^1.20.1",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "eslint": "^7.32.0",
        "express": "^4.18.2",
        "fs": "^0.0.1-security",
        "http-proxy-middleware": "^2.0.6",
        "moment": "^2.29.4",
        "node-gyp": "^9.3.1",
        "npm-run-all": "^4.1.5",
        "regtest": "^0.0.3",
        "rimraf": "^4.4.0",
        "socket.io": "^4.6.1",
        "websocket-extensions": "^0.1.4"
    },
    "engines": {
        "node": "19.x",
        "npm": "8.x",
        "yarn": "3.x"
    },
    "exports": {
        "node": {
            "import": "./dist-server/app.js",
            "require": "./dist-server/app.js"
        }
    },
    "scripts": {
        "build-server": "yarn run clean-server && mkdir ./dist-server && touch ./dist-server/package.json && bash fix-package-type.sh && yarn run transpile-server",
        "run-server": "node ./dist-server/app.js",
        "watch:dev": "nodemon",
        "clean-server": "rimraf dist-server",
        "transpile-server": "babel ./ --out-dir dist-server",
        "prod": "NODE_ENV=production npm-run-all build-server run-server",
        "start": "yarn run prod",
        "test": "jest"
    },
    "nodemonConfig": {
        "exec": "yarn run dev",
        "watch": [
            "server/*"
        ],
        "ignore": [
            "**/__tests__/**",
            "*.test.js",
            "*.spec.js"
        ]
    },
    "packageManager": "yarn@3.4.1",
    "devDependencies": {
        "babel-jest": "^29.5.0",
        "babel-plugin-cjs-esm-interop": "^3.0.0",
        "babel-plugin-module-resolver": "^5.0.0",
        "jest": "^29.5.0",
        "nodemon": "^2.0.21"
    },
    "type": "module"
}
